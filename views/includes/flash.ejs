<% if(Smsg && Smsg.length) { %>
    <div class="alert alert-success alert-dismissible fade show col-6 offset-3" role="alert"> <%= Smsg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
<% } %>

<% if(Emsg && Emsg.length) { %>
    <div class="alert alert-danger alert-dismissible fade show col-6 offset-3" role="alert"> <%= Emsg %>
        <button type="button" class="btn-close" data-bs-dismiss="alert" aria-label="Close"></button>
        </div>
<% } %>